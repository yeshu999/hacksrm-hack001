    private void BB1ActionPerformed(java.awt.event.ActionEvent evt) {                                    
        // TODO add your handling code here:
        new PROFILE().setVisible(true);
        this.setVisible(false);
        
    }                                   

       try
    {
      // create our mysql database connection
      String myDriver = "org.gjt.mm.mysql.Driver";
      String myUrl = "jdbc:mysql://localhost/test";
      Class.forName(myDriver);
      Connection conn = DriverManager.getConnection(myUrl, "root", "");
      
      // our SQL SELECT query. 
      // if you only need a few columns, specify them by name instead of using "*"
      String query = "SELECT * FROM users";

      // create the java statement
      Statement st = (Statement) conn.createStatement();
      
      // execute the query, and get a java resultset
      String a=AT1.getText();
      ResultSet rs = (ResultSet) st.executeQuery("select*from profie2 where name="'+a+"'");
      
      // iterate through the java resultset
      while (rs.next())
      {
          String l = rs.getString(2);
          String h = rs.getString(6);
          String z = rs.getString(7);
          
          String b = rs.getString(3);
          String g = rs.getString(4);
          
AT2.setText(h);
AT6.setText(l);
AT7.setText(z);
AT3.setText(b);
AT4.setText(g);

        
        // print the results
        System.out.format("%s, %s, %s, %s, %s, %s\n", id, firstName, lastName, dateCreated, isAdmin, numPoints);
      }
      st.close();
    }
    catch (Exception e)
    {
      System.err.println("Got an exception! ");
      System.err.println(e.getMessage());
    }
  }
}


