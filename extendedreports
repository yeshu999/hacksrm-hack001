        new PROFILE().setVisible(true);
        this.setVisible(false);DefaultTableModel
t=(DefaultTableModel)jTable1.getModel(); try
{
Class.forName("java.sql.DriverManager");
Connection
c=(Connection)DriverManager.getConnection("jdbc:mysql://localhost/hack_001","root","manager");
Statement
s=(Statement)c.createStatement(); ResultSet
r=s.executeQuery("select * from reports");
while(r.next())
{
String
n=r.getString(1);
String
d=r.getString(2);
String
p=r.getString(3);
String
ci=r.getString(4);
t.addRow(new Object[]{n,d,p,ci});
}


}catch(Exception e)
{
